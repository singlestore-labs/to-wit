//////////////////////////////////////////////////////////////////////////////
//
// This file contains examples of a few different types of interfaces.
//
//////////////////////////////////////////////////////////////////////////////

record polarity-scores {
    compound: float64,
    positive: float64,
    negative: float64,
    neutral: float64,
}

sentiment: function(input: string) -> polarity-scores

//////////////////////////////////////////////////////////////////////////////

square: function(input: s32) -> s32

//////////////////////////////////////////////////////////////////////////////

record split-input {
    s: string,
    delimiter: string,
}

record split-output {
    c: string,
}

split: function(input: split-input) -> list<split-output>

//////////////////////////////////////////////////////////////////////////////

record hilbert-input {
    vec: list<u8>,
    min-value: float64,
    max-value: float64,
    scale: float64,
}

hilbert-encode: function(input: hilbert-input) -> string

